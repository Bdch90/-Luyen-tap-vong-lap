<body>
<script type="text/javascript">
	var tong_so= parseInt(prompt('Nhập vào số lượng số Fibonacci muốn biết :'));
	var dem_so =0;
	var x=0;
	document.write('0 1 ');
	for (var y =1;dem_so<tong_so-2;y=z) {
		var z=x+y;
		document.write(z+' ');
		dem_so++;
		x=y;
	}

</script>
</body>
